                                                    <!-- title row -->
                                                    <div class="row">
                                                        <div class="col-12">
                                                            <h4>
                                                                Evento @Model.eventName 
                                                                <br>
                                                                Fecha del evento: @Model.date - @Model.dateEnd
                                                                <small class="float-right">Fecha de creación:   @Model.date<br></small>

                                                            </h4>
                                                        </div>
                                                        <!-- /.col -->
                                                    </div>
                                                    <!-- info row -->
                                                    <div class="row invoice-info">
                                                        <div class="col-sm-4 invoice-col">
                                                            Número de Empleado:  @Model.numEmploye<br>
                                                            Nombre:   @Model.name<br>
                                                            Empresa:  @Model.compañia<br>
                                                            Persona que autoriza:   @Model.autorizationName<br>
                                                            Número de personas:   @Model.numberPeople<br>
                                                            Lugar del evento:   @Model.locate<br>
                                                            Acomodo:   @Model.accommodation<br>
                                                            Centro de costos:  @Model.costs<br>
                                                            Contacto para logística:  @Model.logistics<br>
                                                            Servicios:
                                                            <br>
                                                            <ul>
                                                                @foreach (var service in Model.serviceEvent)
                                                                {
                                                                    <li>@Html.DisplayFor(modelItem => service)</li>
                                                                }
                                                            </ul>
                                                            <br>
                                                            Comentarios: <br>
                                                            @Model.comments
                                                            <br><br>

                                                        </div>

                                                        <div class="col-sm-8 invoice-col">

                                                        </div>


                                                        <!-- /.col -->
                                                    </div>
                                                    <!-- /.row -->




                                                    <div class="row">
                                                        <!-- accepted payments column -->
                                                        <div class="col-6">

                                                        </div>
                                                        <!-- /.col -->
                                                        <div class="col-6">

                                                        </div>
                                                        <!-- /.col -->
                                                    </div>
                                                    <!-- /.row -->
